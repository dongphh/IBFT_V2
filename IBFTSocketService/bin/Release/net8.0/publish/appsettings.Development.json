{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information",
      "Oracle.ManagedDataAccess": "Debug"
    }
  },
  "SocketServer": {
    "ListenPort": 5000,
    "MaxConnections": 100,
    "ReceiveBufferSize": 8192,
    "SendBufferSize": 8192,
    "ConnectionTimeout": 30000,
    "IdleTimeout": 600000,
    "MaxRequestSize": 1048576
  },
  "OraclePool": {
    "MaxPoolSize": 20,
    "MinPoolSize": 5,
    "IncrPoolSize": 2,
    "DecrPoolSize": 1,
    "ConnectionLifetime": 600,
    "ConnectionIdleTimeout": 300,
    "NumberOfPools": 2,
    "ValidateConnection": true,
    "CommandTimeout": 60
  }
}